{
  "presets": [
    ["latest", { "es2015": { "modules": false } }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    ["transform-object-rest-spread", { "useBuiltIns": true }]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "plugins": [
        "babel-plugin-transform-es2015-modules-umd",
        "istanbul"
      ]
    }
  }
}
